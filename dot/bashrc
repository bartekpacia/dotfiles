#!/usr/bin/env bash

# Run cross shell configuration
source ~/.shrc

if [ -f "$(brew --prefix)/etc/bash_completion" ]; then
    . "$(brew --prefix)/etc/bash_completion"
fi

# Enable searching shell history with fzf
if [ -f ~/.fzf.bash ]; then
    . ~/.fzf.bash
fi

source "$HOME/.dotfiles/script/aliases"
