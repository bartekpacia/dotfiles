alias g="git"

alias gaa="git add ."
alias gb="git branch"
alias gc="git commit"
alias gch="git checkout"
alias gd="git diff"
alias gp="git push"
alias gs="git status"
alias gmnff="git merge --no-ff"
alias lsl="ls -lah"
alias lso="ls -oah"
alias cls="clear"
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
